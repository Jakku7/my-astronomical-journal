ation: orbit 18s linear infinite;\n  transform-style: preserve-3d;\n}\n.mars-orbit {\n  border-radius: 50%;\n  width: 300px;\n  height: 300px;\n  animation: orbit 34s linear infinite;\n  transform-style: preserve-3d;\n}\n.jupiter-orbit {\n  border-radius: 50%;\n  width: 400px;\n  height: 400px;\n  animation: orbit 216s linear infinite;\n  transform-style: preserve-3d;\n}\n.saturn-orbit {\n  border-radius: 50%;\n  width: 540px;\n  height: 540px;\n  animation: orbit 522s linear infinite;\n  transform-style: preserve-3d;\n}\n.uranus-orbit {\n  border-radius: 50%;\n  width: 630px;\n  height: 630px;\n  animation: orbit 1512s linear infinite;\n  transform-style: preserve-3d;\n}\n.neptune-orbit {\n  border-radius: 50%;\n  width: 735px;\n  height: 735px;\n  animation: orbit 2970s linear infinite;\n  transform-style: preserve-3d;\n}\n.planet {\n  position: absolute;\n  left: 50%;\n  top: 50%;\n  transform: translate(-50%, -50%);\n  transition: 0.3s ease-in-out;\n}\n/* .planet-hover {\n  padding: 8px;\n  display: flex;\n  border-radius: 50%;\n  justify-content: center;\n  align-items: center;\n  align-content: center;\n} */\n/* .planet-hover > img:hover {\n  background: #2a3c54;\n  padding: 12px;\n  border-radius: 50%;\n  z-index: 999999;\n} */\n.sun {\n  position: absolute;\n  left: 50%;\n  top: 50%;\n  width: 90px;\n  height: 90px;\n  transform: translate(-50%, -50%) rotateX(-75deg) rotateY(0) rotateZ(0);\n  transform-style: preserve-3d;\n}\n.mercury {\n  width: 4px;\n  height: 4px;\n  left: calc(50% + 75px);\n  transform-style: preserve-3d;\n  transform: translate(-50%, -50%) rotateX(90deg) rotateY(90deg) rotateZ(0);\n  display: flex;\n}\n.mercury-picture {\n  animation: rotation3d 2.25s linear infinite;\n}\n.venus {\n  width: 15px;\n  height: 15px;\n  left: calc(50% + 100px);\n  transform-style: preserve-3d;\n  transform: translate(-50%, -50%) rotateX(90deg) rotateY(90deg) rotateZ(0);\n  display: flex;\n}\n.venus-picture {\n  animation: rotation3d 5.5s linear infinite;\n}\n.earth {\n  width: 17px;\n  height: 17px;\n  left: calc(50% + 120px);\n  transform-style: preserve-3d;\n  transform: translate(-50%, -50%) rotateX(90deg) rotateY(90deg) rotateZ(0);\n  display: flex;\n}\n.earth-picture {\n  animation: rotation3d 9s linear infinite;\n}\n.mars {\n  width: 10px;\n  height: 10px;\n  left: calc(50% + 150px);\n  transform-style: preserve-3d;\n  transform: translate(-50%, -50%) rotateX(90deg) rotateY(90deg) rotateZ(0);\n  display: flex;\n}\n.mars-picture {\n  animation: rotation3d 17s linear infinite;\n}\n.jupiter {\n  width: 50px;\n  height: 50px;\n  left: calc(50% + 200px);\n  transform-style: preserve-3d;\n  transform: translate(-50%, -50%) rotateX(90deg) rotateY(90deg) rotateZ(0);\n  display: flex;\n}\n.jupiter-picture {\n  animation: rotation3d 108s linear infinite;\n}\n.saturn {\n  width: 40px;\n  height: 40px;\n  left: calc(50% + 270px);\n  transform-style: preserve-3d;\n  transform: translate(-50%, -50%) rotateX(90deg) rotateY(90deg) rotateZ(0);\n  display: flex;\n}\n.saturn-picture {\n  animation: rotation3d 261s linear infinite;\n}\n.uranus {\n  width: 30px;\n  height: 30px;\n  left: calc(50% + 320px);\n  transform-style: preserve-3d;\n  transform: translate(-50%, -50%) rotateX(90deg) rotateY(90deg) rotateZ(0);\n  display: flex;\n}\n.uranus-picture {\n  animation: rotation3d 756s linear infinite;\n}\n.neptune {\n  width: 25px;\n  height: 25px;\n  left: calc(50% + 370px);\n  transform-style: preserve-3d;\n  transform: translate(-50%, -50%) rotateX(90deg) rotateY(90deg) rotateZ(0);\n  display: flex;\n}\n.neptune-picture {\n  animation: rotation3d 1485s linear infinite;\n}\n@keyframes orbit {\n  0% {\n    transform: translate3d(-50%, -50%, 0) rotateZ(0);\n  }\n  100% {\n    transform: translate3d(-50%, -50%, 0) rotateZ(-360deg);\n  }\n}\n\n@keyframes orbit3d {\n  0% {\n    transform: translate3d(-50%, -50%, 0) rotateZ(0);\n  }\n  100% {\n    transform: translate3d(-50%, -50%, 0) rotateZ(-360deg);\n  }\n}\n@keyframes rotation3d {\n 